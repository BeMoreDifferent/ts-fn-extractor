{
  "name": "ts-fn-extractor",
  "version": "0.1.0",
  "description": "CLI TS tool that scans a TS project, finds functions, and prints a compact text report",
  "type": "module",
  "bin": {
    "ts-fn-extractor": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/cli.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "keywords": ["typescript", "analyzer", "cli", "functions"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "eslint": "^9.39.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5"
  }
}
