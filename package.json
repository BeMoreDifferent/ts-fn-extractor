    "llm",
    "chatgpt",
    "copilot",
    "cursor",
    "typescript",
    "cli",
    "analyzer",
    "ast",
    "functions",
    "extractor",
    "code-analysis",
    "typescript-compiler-api",
    "ai-tools",
    "developer-tools"
  ],
  "author": "BeMoreDifferent",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/BeMoreDifferent/ts-fn-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/BeMoreDifferent/ts-fn-extractor/issues"
  },
  "homepage": "https://github.com/BeMoreDifferent/ts-fn-extractor#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "eslint": "^9.39.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5"
  }
